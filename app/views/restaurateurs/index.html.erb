<p id="notice"><%= notice %></p>

<h1>Listing Restaurateurs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @restaurateurs.each do |restaurateur| %>
      <tr>
        <td><%= restaurateur.user_id %></td>
        <td><%= restaurateur.active %></td>
        <td><%= link_to 'Show', restaurateur %></td>
        <td><%= link_to 'Edit', edit_restaurateur_path(restaurateur) %></td>
        <td><%= link_to 'Destroy', restaurateur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Restaurateur', new_restaurateur_path %>
